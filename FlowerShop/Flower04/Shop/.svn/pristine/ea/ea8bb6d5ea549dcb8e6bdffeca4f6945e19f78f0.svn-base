package com.po;

import org.springframework.web.multipart.MultipartFile;

public class Goods {
	private Integer id;//商品id
	private String gname;//名称
	private Double goprice;//原价
	private Double gdprice;//折扣价
	private String gunit;//计量单位
	private Integer gstore;//库存
	private MultipartFile logoImage;//封装被上传的文件信息
	private String gpicture;//图片名称
	private Integer goodstype_tid;//商品类型tid
	private String typename;//类型名称，查询时使用
	private Integer gdnow;//折扣是否正在进行，1为正在进行，0为已结束，
	private Integer gfocusnum;//被关注数
	
	public Integer getGfocusnum() {
		return gfocusnum;
	}
	public void setGfocusnum(Integer gfocusnum) {
		this.gfocusnum = gfocusnum;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getGname() {
		return gname;
	}
	public void setGname(String gname) {
		this.gname = gname;
	}
	public Double getGoprice() {
		return goprice;
	}
	public void setGoprice(Double goprice) {
		this.goprice = goprice;
	}
	public Double getGdprice() {
		return gdprice;
	}
	public void setGdprice(Double gdprice) {
		this.gdprice = gdprice;
	}
	public String getGunit() {
		return gunit;
	}
	public void setGunit(String gunit) {
		this.gunit = gunit;
	}
	public Integer getGstore() {
		return gstore;
	}
	public void setGstore(Integer gstore) {
		this.gstore = gstore;
	}
	public MultipartFile getLogoImage() {
		return logoImage;
	}
	public void setLogoImage(MultipartFile logoImage) {
		this.logoImage = logoImage;
	}
	public String getGpicture() {
		return gpicture;
	}
	public void setGpicture(String gpicture) {
		this.gpicture = gpicture;
	}
	public Integer getGoodstype_tid() {
		return goodstype_tid;
	}
	public void setGoodstype_tid(Integer goodstype_tid) {
		this.goodstype_tid = goodstype_tid;
	}
	public String getTypename() {
		return typename;
	}
	public void setTypename(String typename) {
		this.typename = typename;
	}
	public Integer getGdnow() {
		return gdnow;
	}
	public void setGdnow(Integer gdnow) {
		this.gdnow = gdnow;
	}
}
