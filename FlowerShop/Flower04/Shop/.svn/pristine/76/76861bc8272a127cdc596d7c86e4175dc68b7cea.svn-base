package com.po;

public class Buser {
	private Integer id;//用户id
	private String bphone;//手机号
	private String bpwd;//密码
	private String bfocus;//用户关注的商品tid
	private Integer bcanlogin;//用户是否被限制登录，1表示未限制，0表示限制
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getBphone() {
		return bphone;
	}
	public void setBphone(String bphone) {
		this.bphone = bphone;
	}
	public String getBpwd() {
		return bpwd;
	}
	public void setBpwd(String bpwd) {
		this.bpwd = bpwd;
	}
	public String getBfocus() {
		return bfocus;
	}
	public void setBfocus(String bfocus) {
		this.bfocus = bfocus;
	}
	public Integer getBcanlogin() {
		return bcanlogin;
	}
	public void setBcanlogin(Integer bcanlogin) {
		this.bcanlogin = bcanlogin;
	}
}
